<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel border="true" bottom="-409" enabled="true" follows="left|top|right|bottom"
     height="408" hidden="false" label="Text Chat" left="102" mouse_opaque="true"
     name="chat" width="517">
	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-20" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box" v_pad="0" width="128">
		Chat Font Size:
	</text>
	<radio_group bottom="-72" control_name="ChatFontSize" draw_border="true" enabled="true"
	     follows="left|top" height="60" hidden="false" left="148"
	     mouse_opaque="true" name="chat font size" width="128">
		<radio_item type="string" length="1" bottom="-20" enabled="true" height="16" hidden="false" left="0"
		     mouse_opaque="true" name="radio" width="98">
			Small
		</radio_item>
		<radio_item type="string" length="1" bottom="-40" enabled="true" height="16" hidden="false" left="0"
		     mouse_opaque="true" name="radio2" width="98">
			Medium
		</radio_item>
		<radio_item type="string" length="1" bottom="-60" enabled="true" height="16" hidden="false" left="0"
		     mouse_opaque="true" name="radio3" width="98">
			Large
		</radio_item>
	</radio_group>
	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-95" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box2" v_pad="0" width="128">
		Chat Color:
	</text>
	
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="0.8 1 1 1"
	     control_name="SystemChatColor" enabled="true" follows="left|top"
	     height="56" hidden="false" label="System" left="148" mouse_opaque="true"
	     name="system" width="48" />
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="1 1 1 1" control_name="AgentChatColor"
	     enabled="true" follows="left|top" height="56" hidden="false" label="Users"
	     left_delta="54" mouse_opaque="true" name="users" width="48" />
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="0.7 0.9 0.7 1"
	     control_name="ObjectChatColor" enabled="true" follows="left|top"
	     height="56" hidden="false" label="Objects" left_delta="54" mouse_opaque="true"
	     name="objects" width="48" />
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="0.7 0.9 0.7 1"
	     control_name="llOwnerSayChatColor" enabled="true" follows="left|top"
	     height="56" hidden="false" label="Owner" left_delta="54" mouse_opaque="true"
	     name="owner" width="48" />	
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="0 0 0 1"
	     control_name="BackgroundChatColor" enabled="true" follows="left|top"
	     height="56" hidden="false" label="Bubble" left_delta="54" mouse_opaque="true"
	     name="background" width="48" />
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-140"
	     can_apply_immediately="true" color="0.6 0.6 1 1"
	     control_name="HTMLLinkColor" enabled="true" follows="left|top" height="56"
	     hidden="false" label="URLs" left_delta="54" mouse_opaque="true" name="links"
	     width="48" />
	     
	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-160" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box3" v_pad="0" width="135">
		Chat Console:
	</text>
	<spinner bottom_delta="-6" control_name="ChatPersistTime" decimal_digits="0" enabled="true"
	     follows="left|top" height="16" hidden="false" increment="1"
	     initial_val="10" label="Fade chat after" label_width="80" left="148"
	     max_val="60" min_val="2" mouse_opaque="true" name="fade_chat_time"
	     width="130" />
	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="6" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="282" mouse_opaque="false" name="text_box4" v_pad="0" width="80">
		(seconds)
	</text>
	<spinner bottom_delta="-6" control_name="ConsoleMaxLines" decimal_digits="0" enabled="true"
	     follows="left|top" height="16" hidden="false" increment="1"
	     initial_val="10" left="340" max_val="50" min_val="1" mouse_opaque="true"
	     name="max_chat_count" width="60" />
	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="6" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="405" mouse_opaque="false" name="text_box5" v_pad="0" width="60">
		(# lines)
	</text>
	<slider bottom="-185" can_edit_text="false" control_name="ConsoleBackgroundOpacity"
	     decimal_digits="3" enabled="true" follows="left|top" height="12"
	     hidden="false" increment="0.05" initial_val="1" label="Opacity" left="148"
	     max_val="1" min_val="0" mouse_opaque="true" name="console_opacity"
	     show_text="true" value="0.4" width="200" />

	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-207" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box6" v_pad="0" width="135">
		Chat Options:
	</text>
	<check_box bottom_delta="-6" control_name="ChatFullWidth" enabled="true" follows="left|top"
	     font="SansSerifSmall" height="16" hidden="false" initial_value="false"
	     label="Chat uses full screen width (after restart)" left="148"
	     mouse_opaque="true" name="chat_full_width_check" radio_style="false"
	     width="239" />
	<check_box bottom_delta="-20" control_name="CloseChatOnReturn" enabled="true"
	     follows="left|top" font="SansSerifSmall" height="16" hidden="false"
	     initial_value="false" label="Close chat after hitting return" left="148"
	     mouse_opaque="true" name="close_chat_on_return_check" radio_style="false"
	     width="237" />
	<check_box bottom_delta="-20" control_name="ArrowKeysMoveAvatar" enabled="true"
	     follows="left|top" font="SansSerifSmall" height="16" hidden="false"
	     initial_value="false" label="Arrow keys always move avatar when chatting"
	     left="148" mouse_opaque="true" name="arrow_keys_move_avatar_check"
	     radio_style="false" width="237" />
	<check_box bottom_delta="-20" control_name="ChatShowTimestamps" enabled="true"
	     follows="left|top" font="SansSerifSmall" height="16" hidden="false"
	     initial_value="false" label="Show timestamps in chat" left="148"
	     mouse_opaque="true" name="show_timestamps_check" radio_style="false"
	     width="237" />

	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-292" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box7" v_pad="0" width="128">
		Bubble Chat:
	</text>
	<check_box bottom="-299" control_name="UseChatBubbles" enabled="true" follows="left|top"
	     font="SansSerifSmall" height="16" hidden="false" initial_value="false"
	     label="Show chat bubbles" left="148" mouse_opaque="true"
	     name="bubble_text_chat" radio_style="false" width="237" />
	<slider bottom="-315" can_edit_text="false" control_name="ChatBubbleOpacity"
	     decimal_digits="3" enabled="true" follows="left|top" height="12"
	     hidden="false" increment="0.05" initial_val="1" label="Opacity" left="148"
	     max_val="1" min_val="0" mouse_opaque="true" name="bubble_chat_opacity"
	     show_text="true" value="0.5" width="200" />

	<text type="string" length="1" bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom="-339" drop_shadow_visible="true" enabled="true" follows="left|top"
	     font="SansSerifSmall" h_pad="0" halign="left" height="10" hidden="false"
	     left="12" mouse_opaque="false" name="text_box8" v_pad="0" width="128">
		Script Errors:
	</text>
	<check_box bottom="-346" control_name="ScriptErrorsAsChat" enabled="true"
	     follows="left|top" font="SansSerifSmall" height="16" hidden="false"
	     initial_value="false"
	     label="Show script errors and warnings as regular chat" left="148"
	     mouse_opaque="true" name="script_errors_as_chat" radio_style="false"
	     width="275" />
	<color_swatch border_color="0.45098 0.517647 0.607843 1" bottom="-403"
	     can_apply_immediately="true" color="0.82 0.82 0.99 1"
	     control_name="ScriptErrorColor" enabled="true" follows="left|top"
	     height="56" hidden="false" label="Color" left="148" mouse_opaque="true"
	     name="script_error" width="48" />
</panel>
